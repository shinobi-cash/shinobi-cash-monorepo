{
  "name": "@shinobi-cash/core",
  "version": "0.0.1",
  "description": "Core SDK for Shinobi Cash - note generation, discovery, and ZK proof operations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "shinobi-cash",
    "privacy",
    "zk-proof",
    "notes",
    "cryptography",
    "poseidon"
  ],
  "license": "MIT",
  "dependencies": {
    "@shinobi-cash/constants": "workspace:*",
    "@shinobi-cash/data": "workspace:*",
    "@zk-kit/lean-imt": "^2.2.4",
    "bip39": "^3.1.0",
    "ethers": "^6.13.4",
    "poseidon-lite": "^0.3.0",
    "snarkjs": "^0.7.5",
    "viem": "^2.21.54"
  },
  "devDependencies": {
    "@shinobi-cash/eslint-config": "workspace:*",
    "@shinobi-cash/typescript-config": "workspace:*",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}