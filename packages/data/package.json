{
  "name": "@shinobi-cash/data",
  "version": "0.0.1",
  "description": "Data layer for Shinobi Cash with fluent query builders",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist rollup-dist",
    "analyze": "rollup -c rollup.analyze.mjs && open rollup-dist/bundle-analysis.html"
  },
  "keywords": [
    "subgraph",
    "graphql",
    "data-layer",
    "ethereum",
    "query-builder"
  ],
  "license": "MIT",
  "dependencies": {
    "@shinobi-cash/constants": "workspace:*",
    "graphql-request": "7.2.0"
  },
  "devDependencies": {
    "@shinobi-cash/eslint-config": "workspace:*",
    "@shinobi-cash/typescript-config": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "eslint": "^9.17.0",
    "rollup": "^4.46.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}