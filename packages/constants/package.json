{
  "name": "@shinobi-cash/constants",
  "version": "0.0.1",
  "description": "Shared constants for shinobi cash packages",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "clean": "rm -rf dist rollup-dist",
    "analyze": "rollup -c rollup.analyze.mjs && open rollup-dist/bundle-analysis.html"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "devDependencies": {
    "@shinobi-cash/eslint-config": "workspace:*",
    "@shinobi-cash/typescript-config": "workspace:*",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "eslint": "^9.17.0",
    "rollup": "^4.28.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "viem": "^2.28.1",
    "vitest": "^2.1.8"
  }
}